<web-app>
	<display-name>sgp</display-name>
	<servlet>

		<servlet-name>ListerCollaborateursController</servlet-name>
		<servlet-class>
			dev.spg.web.ListerCollaborateursController
		</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ListerCollaborateursController</servlet-name>
		<url-pattern>/collaborateurs/lister</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>EditerCollaborateurController</servlet-name>
		<servlet-class>dev.spg.web.EditerCollaborateurController
		</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>EditerCollaborateurController</servlet-name>
		<url-pattern>/collaborateurs/editer</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>CreationCollaborateurControlleur</servlet-name>
		<servlet-class>dev.spg.web.CreationCollaborateurControlleur
		</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CreationCollaborateurControlleur</servlet-name>
		<url-pattern>/collaborateurs/creation</url-pattern>
	</servlet-mapping>



</web-app>